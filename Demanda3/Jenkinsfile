pipeline {
    agent any

    stages {
        stage('SCM Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/Franck-Patrick/Desafio_DellTech.git'
            }
        }
    }

    stages {
        stage('Build Docker Image') {
            steps {
                sh 'dcoker build -t pkdu14/webapp:$BUILD_NUMBER .'
            }
        }
    }
}
